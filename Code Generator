g=input("Enter the total likes for facebook:")
h=input("Enter the total comments for facebook:")
i=input("Enter the total shares for facebook:")
j=input("Enter the total likes for twitter:")
k=input("Enter the total comments for twitter:")
l=input("Enter the total shares for twitter:")
if g.isnumeric() and h.isnumeric() and i.isnumeric() and j.isnumeric() and k.isnumeric() and l.isnumeric():print("These are numeric numbers")
else:print("it is invalid")
x=int(input("Enter the total likes for facebook:"))
y=int(input("Enter the total comments for facebook:"))
z=int(input("Enter the total shares for facebook:"))
a=int(input("Enter the total likes for twitter:"))
b=int(input("Enter the total comments for twitter:"))
c=int(input("Enter the total shares for twitter:"))
d=x+y+z
e=a+b+c
f=d+e
if (x<0) or (y<0) or (z<0) or (a<0) or (b<0) or (c<0):print("it is a negative number")
elif (d>e):print("Facebook has higher engagement than Twitter")
elif(d<e):print("Twitter has higher engagement than Facebook")
elif(x<0) or (y<0) or (z<0) or (a<0) or (b<0) or (c<0):print("it is a negative number")
elif(d==e):print("Twitter has same engagement than Facebook")
if(f>200):print("Higher Engagement")
elif(f<200):print("Lower Engagemnet")
